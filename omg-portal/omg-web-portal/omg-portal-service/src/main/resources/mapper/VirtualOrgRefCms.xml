<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD SQL Map 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.uce.omg.portal.dao.IVirtualOrgRefCmsDao">
	
	<select id="findVirtualOrgRefCmsRef" resultType="cn.uce.omg.portal.vo.VirtualOrgRefCmsVo">
		SELECT 
		VIRTUAL_BASE_ORG_CODE,
		CMS_BASE_ORG_CODE
		FROM 
		OMG_VIRTUAL_ORG_REF_CMS
		WHERE DELETE_FLAG = 0
	</select>

</mapper>
