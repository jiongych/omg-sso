<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC 
	"-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.uce.omg.sso.dao.IFspDictDataDao">

	<!-- 根据typeClassCode查询数据字典 -->
	<select id="findListByTypeClassCode" parameterType="java.lang.String" resultType="sysTypeVo">
		SELECT * FROM OMG_FSP_DICT_DATA
		WHERE
		TYPE_CLASS_CODE = #{_parameter} 
		AND DELETE_FLAG <![CDATA[ <> ]]> 1
	</select>
	
</mapper>