<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC 
	"-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.uce.omg.sso.dao.IOmgWeakPasswordDao">
	<sql id="Base_Column_List">
			ID,WEAK_PLAINTEXT_PASSWORD,WEAK_ENCRYPT_PASSWORD,
			CREATE_TIME
	</sql>
	<select id="findByWeakEncryptPassword" parameterType="java.lang.String" resultType="cn.uce.omg.sso.entity.OmgWeakPassword">
		select <include refid="Base_Column_List" /> from omg_weak_password
		where WEAK_ENCRYPT_PASSWORD = #{_parameter}
	</select>
</mapper>