<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD SQL Map 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.uce.omg.sso.dao.ILoginItemDao">
	<insert id="insertItem" parameterType="LoginItemVo">
		INSERT INTO OMG_LOGIN_ITEM(OPERATE_TYPE,EMP_ID,USER_NAME,SYSTEM_CODE,OPERATE_TIME,IP_ADDR,MACHINE_CODE,VERSION,CREATE_TIME,UPDATE_TIME,ERROR_CODE) 
		VALUES ( #{operateType},#{empId},#{userName},#{systemCode},#{operateTime},#{ipAddr},#{machineCode},#{version},#{createTime},#{updateTime},
		<if test="errorCode == null">
       		''
       	</if>
       	<if test="errorCode != null">
       		#{errorCode}
       	</if>
		)
	</insert>
</mapper>
